<resources>

    <!-- Base application theme. NoActionBar无标题栏-->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <!-- 无状态栏 -->
        <item name="android:windowFullscreen">true</item>
        <!-- 启动程序会启动一个空白窗口，设置和闪屏页一样的图片，这样视觉上就能顺畅的过度到闪屏页 -->
        <item name="android:windowBackground">@color/color00574B</item>
    </style>

</resources>
